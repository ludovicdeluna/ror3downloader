<h1>Listing sessions</h1>

<table>
  <tr>
    <th>id</th>
    <th>Session Id</th>
    <th>Data</th>
    <th>Updated At</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sessions.each do |session| %>
  <tr>
    <td><%= session.id %></td>
    <td><%= session.session_id %></td>
    <td><%= Marshal.load(Base64.decode64(session.data)) %></td>
    <td><%= session.updated_at %></td>
    <td><%= link_to 'Show', session %></td>
    <td><%= link_to( 'Edit', edit_session_path(session) ) %></td>
    <td><%= link_to 'Destroy', session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
<br />

<%= link_to 'Clean now', clean_sessions_path, method: :post %>
<%= link_to( 'New Session', new_session_path ) if false %>
<%= link_to 'Back', medias_path %>
